{
	"name": "cookied",
	"version": "0.1.0",
	"description": "Cookied Monorepo",
	"main": "index.js",
	"repository": "https://github.com/ashuvssut/cookied",
	"author": "Team Cookied",
	"private": true,
	"scripts": {
		"metro": "yarn workspace client dev",
		"web": "yarn workspace client web",
		"android": "yarn workspace client android",
		"apk": "cd ./apps/client/android && ./gradlew assembleRelease",
		"apk-install": "adb install ./apps/client/android/app/build/outputs/apk/release/app-release.apk",
		"reset-cache": "yarn workspace client reset-cache",
		"gradle-clean": "yarn workspace client gradle-clean",
		"ext": "yarn workspace ext dev",
		"new-fx": "yarn workspace api new",
		"deploy-fx": "yarn workspace api deploy"
	},
	"workspaces": [
		"packages/*",
		"apps/chrome-extension/",
		"apps/client/",
		"apps/api/",
		"apps/api/functions/*"
	],
	"dependencies": {}
}
